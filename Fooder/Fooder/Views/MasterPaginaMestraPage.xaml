        <local:MenuLateralPage x:Name="masterPage"></local:MenuLateralPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:DadosIniciaisPage></local:DadosIniciaisPage>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>