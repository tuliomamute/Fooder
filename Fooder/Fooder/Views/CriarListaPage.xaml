<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fooder.Views.CriarListaPage"
             Title="Criar Lista">
    <ScrollView>
        <StackLayout Padding="10">
            <Entry Text="{Binding ObjLista.NomeLista}" Placeholder="Nome Lista"></Entry>
            <Button Command="{Binding Navigate}" Text="Salvar"></Button>
        </StackLayout>
    </ScrollView>
</ContentPage>